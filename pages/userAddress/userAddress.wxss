/**index.wxss**/
page{
	min-height:100%;
	width:100%;
	background:#f2f2f2;

}
#address{
	width:100%;
	height:100%;
	background:#f5f5f5}
.address_item{
	border-bottom:solid 1px #ececec;
	padding:40rpx 2.6%;
	background:#fff;
	width:94.8%;
	height:100rpx;
}
.address_left{
	width:70%;
	overflow:hidden;
	height:100%;
}
.address_left .text1{
	color:#262626;
	font-size:34rpx;
	}
.text_top{
	height:60%;
}
.text_other{
	height:40%;
	font-size:30rpx;
}
.address_right{
	font-size:30rpx;
	color:#262626;
	width:30%;
	overflow:hidden;
	text-align:right;
	height:100%;
}
.address_set{
	padding:20px 25rpx;
	border-bottom:solid 1px #ececec;
	background:#fff;
	justify-content:center;
	}
.address_set text{font-size:24rpx; }
.address_set .default{
	color:#550f0f;width:71%;
	align-items:center;
	height:30rpx;
	display:flex;
	align-items:center;
	}
.address_set .default image{
	margin-right:15px;
	width:29rpx;
	height:29rpx;}
.address_set  .edit image{
	width:26rpx;
	height:26rpx;
	margin-right:10rpx;
}
.edit{color:#7d7d7d;width:15%;display:flex;
	align-items:center;}
.de_color{color:#7d7d7d}
.btn{
	position:absolute;
	bottom:120rpx;
	left:106rpx;
}
.submit_info{
	margin:0 auto;

}