<!--userINfor.wxml-->
<view class="w710">
     <view class="user_tlt">
    <view class="tlt_cont"><image src="/images/user_icon6.png"></image>订单确认</view>
    </view>
    <view style="width:100%;background:#262626;border-radius: 10rpx;margin-top: 20rpx;">
        <view class="w710">
            <view class="infor_list flexRowBetween">
                <view class="infor_left">
                    年卡:
                </view>
                <view class="infor_center">
                    ¥200
                </view>
            </view>
           <view class="infor_list flexRowBetween">
                <view class="infor_left">
                    数量:
                </view>
                 <view class="infor_center flexRowBetween" style="width:30%;">
                    <view class="mins"><image src="/images/confirm1.png"></image></view>
                    <view class="number">1</view>
                    <view class="add"><image src="/images/confirm2.png"></image></view>
                </view>
            </view>
             <view class="infor_list flexRowBetween"  bindtap="intoPath" data-path="/pages/userDiscount/userDiscount">
                <view class="infor_left">
                    优惠卷:
                </view>
                <view class="infor_center">
                    暂无可用
                </view>
                <view class="infor_right">
                    <image src="/images/next_page.png"></image>
                </view>
            </view>
            <view class="infor_list" style="border:none;color:#fff;text-align: right;line-height: 100rpx;font-size:32rpx;" >
                实付款  <text style="color:#bb212e;font-size:28rpx;margin-left: 20rpx;">¥923</text>
            </view>
        </view>
    </view>
</view>
<button class="submit_info"  bindtap="intoPathRedi" data-path="/pages/user/user"> 提交订单,微信支付 </button>